    "def encrypt(res, key):\n",
    "    file.seek(0)\n",
    "    for i in range(len(res)):\n",
    "        res[i] = res[i] ^ key[i % len(key)]\n",
    "    return (bytes(res))\n",
    "\n",
    "name = input()\n",
    "key = bytearray(input().encode())\n",
    "\n",
    "file = open(name, 'rb+')\n",
    "file.write(encrypt(bytearray(file.read()), key))\n",
    "file.close()"
   ]
